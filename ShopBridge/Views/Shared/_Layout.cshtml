<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ShopBridge - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/TableStyle")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="row">
                <div class="hdrTop clearfix">
                    <a href="javascript:void(0);" class="logoPart"><img src='@Url.Content("~/images/logo2.PNG")' alt="" /></a>
                    <div class="loging-right-btn">


                        @*<a href="@Url.Action("LogOut", "Account", new { rUrl = TempData["rUrl"] })" class="btn btn-info">Logout</a>*@
                        @*<a href='@Url.Action("LogOut", "Account")'><i class="fa fa-power-off"></i></a>*@
                    </div>
                </div>
                <div class="hdrBtm">
                    @*@Html.Partial("_MainMenuPartial")*@
                </div>

            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @*<hr />*@
        <footer>
            @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/app/ItemList.js")
    @RenderSection("scripts", required: false)
</body>
</html>
